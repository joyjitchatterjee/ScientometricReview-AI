@article{LIU2013954,
	title        = {The vibration analysis of wind turbine blade–cabin–tower coupling system},
	author       = {W.Y. Liu},
	year         = 2013,
	journal      = {Engineering Structures},
	volume       = 56,
	pages        = {954--957},
