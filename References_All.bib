@article{en12244612,
	title        = {A Hybrid Short-Term Load Forecasting Framework with an Attention-Based Encoder–Decoder Network Based on Seasonal and Trend Adjustment},
	author       = {Meng, Zhaorui and Xu, Xianze},
	year         = 2019,
	journal      = {Energies},
	volume       = 12,
	number       = 24,
	doi          = {10.3390/en12244612},
	issn         = {1996-1073},
	url          = {https://www.mdpi.com/1996-1073/12/24/4612},
	article-number = 4612,
	abstract     = {Accurate electrical load forecasting plays an important role in power system operation. An effective load forecasting approach can improve the operation efficiency of a power system. This paper proposes the seasonal and trend adjustment attention encoder&ndash;decoder (STA&ndash;AED), a hybrid short-term load forecasting approach based on a multi-head attention encoder&ndash;decoder module with seasonal and trend adjustment. A seasonal and trend decomposing technique is used to preprocess the original electrical load data. Each decomposed datum is regressed to predict the future electric load value by utilizing the encoder&ndash;decoder network with the multi-head attention mechanism. With the multi-head attention mechanism, STA&ndash;AED can interpret the prediction results more effectively. A large number of experiments and extensive comparisons have been carried out with a load forecasting dataset from the United States. The proposed hybrid STA&ndash;AED model is superior to the other five counterpart models such as random forest, gradient boosting decision tree (GBDT), gated recurrent units (GRUs), Encoder&ndash;Decoder, and Encoder&ndash;Decoder with multi-head attention. The proposed hybrid model shows the best prediction accuracy in 14 out of 15 zones in terms of both root mean square error (RMSE) and mean absolute percentage error (MAPE).}
}
