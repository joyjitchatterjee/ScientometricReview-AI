@inproceedings{gong-etal-2019-enhanced,
	title        = {Enhanced Transformer Model for Data-to-Text Generation},
	author       = {Gong, Li  and Crego, Josep  and Senellart, Jean},
	year         = 2019,
	month        = nov,
	booktitle    = {Proceedings of the 3rd Workshop on Neural Generation and Translation},
	publisher    = {Association for Computational Linguistics},
	address      = {Hong Kong},
	pages        = {148--156},
	abstract     = {Neural models have recently shown significant progress on data-to-text generation tasks in which descriptive texts are generated conditioned on database records. In this work, we present a new Transformer-based data-to-text generation model which learns content selection and summary generation in an end-to-end fashion. We introduce two extensions to the baseline transformer model: First, we modify the latent representation of the input, which helps to significantly improve the content correctness of the output summary; Second, we include an additional learning objective that accounts for content selection modelling. In addition, we propose two data augmentation methods that succeed to further improve performance of the resulting generation models. Evaluation experiments show that our final model outperforms current state-of-the-art systems as measured by different metrics: BLEU, content selection precision and content ordering. We made publicly available the transformer extension presented in this paper.}
}
@article{RePEc:eee:rensus:v:98:y:2018:i:c:p:189-198,
	title        = {{Deep Learning for fault detection in wind turbines}},
	author       = {Helbing, Georg and Ritter, Matthias},
	year         = 2018,
	month        = {},
	journal      = {Renewable and Sustainable Energy Reviews},
	volume       = 98,
	number       = {C},
	pages        = {189--198},
	doi          = {10.1016/j.rser.2018.09.01},
	url          = {https://ideas.repec.org/a/eee/rensus/v98y2018icp189-198.html},
	keywords     = {Deep Learning; Artificial neural network; Fault detection; Condition monitoring; Wind turbine},
	abstract     = {Condition monitoring in wind turbines aims at detecting incipient faults at an early stage to improve maintenance. Artificial neural networks are a tool from machine learning that is frequently used for this purpose. Deep Learning is a machine learning paradigm based on deep neural networks that has shown great success at various applications over recent years. In this paper, we review unsupervised and supervised applications of artificial neural networks and in particular of Deep Learning to condition monitoring in wind turbines. We find that – despite a promising performance of supervised methods – unsupervised approaches are prevalent in the literature. To explain this phenomenon, we discuss a range of issues related to obtaining labelled data sets for supervised training, namely quality and access as well as labelling and class imbalance of operational data. Furthermore, we find that the application of Deep Learning to SCADA data is impeded by their relatively low dimensionality, and we suggest ways of working with higher-dimensional SCADA data.}
}
